{
    "name": "maxxscho/laravel-markdown-plus",
    "description": "A simple Laravel Markdown Parser with an optional meta section in the .md file",
    "keywords": ["laravel", "markdown"],
    "license": "MIT",
    "authors": [
        {
            "name": "Markus Schober",
            "email": "maxx.schober@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "michelf/php-markdown": "1.4.*",
        "symfony/yaml": "2.5.*"
    },
    "require-dev": {
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Maxxscho\\LaravelMarkdownPlus\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
